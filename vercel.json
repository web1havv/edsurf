{
  "version": 2,
  "builds": [
    {
      "src": "backend/vercel_entry.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/vercel_entry.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "functions": {
    "backend/vercel_entry.py": {
      "maxDuration": 60
    }
  }
}
